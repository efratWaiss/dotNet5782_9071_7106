<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="800">
    <Grid Background="White">
        <Image Source="drone.jpg" Width="800" Margin="0,-139,0,71"/>
        <Rectangle Fill="#FF01356E" StrokeThickness="2" Margin="0,429,0,179" />
        <Button x:Name="buttonDrone" Content="open drones list"
                Click="Button_ClickDrone" Margin="640,517,15,44" BorderBrush="#FF01356E" BorderThickness="7"
                FontSize="22" FontFamily="Segoe UI Variable Text Semibold" FontWeight="Bold" FontStretch="ExtraCondensed" Visibility="Visible" FontStyle="Italic" Style="{Binding Cursor, ElementName=button}" RenderTransformOrigin="0.5,0.5" >
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.764"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
            <Button.Background>
                <ImageBrush/>
            </Button.Background>
        </Button>
